<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Trigonometry suite">

    <groups>
        <run>
            <exclude name="Negative"/>
        </run>
    </groups>

    <listeners>
        <listener class-name="com.epam.module3.listeners.TestListener"/>
    </listeners>
    
    <test name="Test trigonometrical functions">
        <classes>
            <class name="com.epam.module3.TrigonometryTest"/>
        </classes>
    </test>


</suite>